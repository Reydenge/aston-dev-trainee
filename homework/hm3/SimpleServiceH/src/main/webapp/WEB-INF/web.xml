         version="5.0">

    <display-name>Students</display-name>
    
    <servlet>
        <servlet-name>StudentController</servlet-name>
        <servlet-class>ru.reydenge.simpleserviceh.web.StudentController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentController</servlet-name>
